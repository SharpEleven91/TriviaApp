{
    "root": true,
    "env": {
        "browser": true,
        "mocha": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "extends": ["eslint:recommended", "google"],
    "rules": {
        "linebreak-style": 0,
        "no-tabs": 0,  
        "switch-colon-spacing": 0,
        "quotes": 0 
    }
}